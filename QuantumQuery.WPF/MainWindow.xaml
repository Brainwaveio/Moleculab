<Window x:Class="QuantumQuery.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuantumQuery.WPF"
        mc:Ignorable="d"
        Icon="quantumquery.ico"
        Title="QuantumQuery" Height="900" Width="1600">

    <Grid>
        <TextBlock VerticalAlignment="Top" Width="250" Height="40" Text="{Binding BoundText}" />
        <Button Click="ButtonTest" Width="80" Height="40" />
        <ListView VerticalAlignment="Bottom" Width="300" Height="300" ItemsSource="{Binding Entries}" />
    </Grid>
</Window>
